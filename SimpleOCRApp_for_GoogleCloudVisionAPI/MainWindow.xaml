<Window x:Class="SimpleOCRApp_for_GoogleCloudVisionAPI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleOCRApp_for_GoogleCloudVisionAPI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Load To Image" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" Width="106" Click="button_Click"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="254" Margin="10,55,0,0" VerticalAlignment="Top" Width="208" OpacityMask="#FF555050" />
        <Button x:Name="button1" Content="OCR" HorizontalAlignment="Left" Margin="223,131,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="254" Margin="303,55,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="204"/>

    </Grid>
</Window>
